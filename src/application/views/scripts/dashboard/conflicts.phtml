                <? endif; ?>
                    <div class="grid_2">
                        <span>
                            <?= $conflict->html('module/label') ?>
                        </span>
                    </div>
                    <div class="grid_13 cssLogTxtAlert">
                        <span class="ui-icon ui-icon-alert">ico</span>
                        <span>
                            <? $translateVars = array(); ?>
                            <? foreach($conflict->html('messageVars') as $messageVar): ?>
