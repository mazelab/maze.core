<div class="cssDashboardLogs grid_16">
    <div class="cssToolBoxAdminDiff grid_16">
        <div class="cssToolboxHead">
            <img src="<?= $this->baseUrl() ?>/img/icon_diff_25.png" alt="<?= $this->translate('image') ?>">
